@article{dykstraSDD,
author = {Raydan, Marcos and Tarazaga, Pablo},
title = {Primal and polar approach for computing the symmetric diagonally dominant projection},
journal = {Numerical Linear Algebra with Applications},
volume = {9},
number = {5},
pages = {333-345},
keywords = {diagonally dominant matrices, polyhedral cones, Dykstra's algorithm, Kuhn–Tucker conditions, Moreau's theorem},
doi = {https://doi.org/10.1002/nla.277},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/nla.277},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/nla.277},
abstract = {Abstract We solve the problem of minimizing the distance from a given matrix to the cone of symmetric and diagonally dominant matrices with positive diagonal (SDD+). Using the extreme rays of the polar cone we project onto the supporting hyperplanes of the faces of SDD+ and then, applying the cyclic Dykstra's algorithm, we solve the problem. Similarly, using the extreme rays of SDD+ we characterize the projection onto the polar cone, which also allows us to obtain the projection onto SDD+ by means of the orthogonal decomposition theorem for convex cones. In both cases the symmetry and the sparsity pattern of the given matrix are preserved. Preliminary numerical experiments indicate that the polar approach is a promising one. Copyright © 2002 John Wiley \& Sons, Ltd.},
year = {2002}
}

@article{JSSv060i03,
   author = {Ernesto G. Birgin and Jose Mario Martínez and Marcos Raydan},
   title = {Spectral Projected Gradient Methods: Review and Perspectives},
   journal = {Journal of Statistical Software, Articles},
   volume = {60},
   number = {3},
   year = {2014},
   keywords = {},
   abstract = {Over the last two decades, it has been observed that using the gradient vector as a search direction in large-scale optimization may lead to efficient algorithms. The effectiveness relies on choosing the step lengths according to novel ideas that are related to the spectrum of the underlying local Hessian rather than related to the standard decrease in the objective function. A review of these so-called spectral projected gradient methods for convex constrained optimization is presented. To illustrate the performance of these low-cost schemes, an optimization problem on the set of positive definite matrices is described.},
   issn = {1548-7660},
   pages = {1--21},
   doi = {10.18637/jss.v060.i03},
   url = {https://www.jstatsoft.org/v060/i03}
}

